{
  "name": "atom-ide-datatip",
  "main": "./dist/main.js",
  "version": "0.20.3",
  "description": "A replacement for the Data Tooltip provider that was originally part of the Atom IDE package from Facebook.",
  "keywords": [
    "atom-package",
    "atom-ide",
    "datatip"
  ],
  "repository": "https://github.com/atom-ide-community/atom-ide-datatip",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "test-debug": "npm run debug && atom --test spec",
    "test": "npm run build && atom --test spec",
    "clean": "shx rm -rf dist",
    "debug": "npm run clean && shx cp -r lib dist && cross-env NODE_ENV=development cross-env BABEL_KEEP_MODULES=false babel dist --out-dir dist",
    "dev": "npm run clean && cross-env NODE_ENV=development cross-env BABEL_KEEP_MODULES=true rollup -c -w",
    "build": "npm run clean && cross-env NODE_ENV=production cross-env BABEL_KEEP_MODULES=true rollup -c",
    "build-commit": "build-commit -o dist",
    "bump": "ncu -u",
    "prepare": "npm run build"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "package-deps": [
    "busy-signal"
  ],
  "dependencies": {
    "atom-package-deps": "^6.0.0",
    "atom-ide-base": "^2.1.1"
  },
  "devDependencies": {
    "@types/atom": "^1.40.4",
    "@types/node": "^14.14.2",
    "atom-languageclient": "^1.0.0",
    "@types/jasmine": "^3.5.14",
    "atom-jasmine3-test-runner": "^5.1.4",
    "typescript": "^4.0.3",
    "tslib": "^2.0.3",
    "prettier": "^2.1.2",
    "eslint": "7.11.0",
    "eslint-config-atomic": "^1.5.0",
    "rollup": "^2.32.1",
    "rollup-plugin-atomic": "^1.6.2",
    "shx": "^0.3.2",
    "cross-env": "latest",
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "babel-preset-atomic": "latest",
    "npm-check-updates": "^9.1.2",
    "build-commit": "^0.1.1"
  },
  "atomTestRunner": "./spec/runner",
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "datatip": {
      "versions": {
        "0.1.0": "provideDatatipService"
      }
    }
  }
}
